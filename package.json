{
  "name": "scratch-solid",
  "version": "0.0.1",
  "description": "build solid from scratch",
  "main": "index.js",
  "author": "darthvid",
  "license": "MIT",
  "private": false,
  "scripts": {
    "old:build": "parcel build src/index.html",
    "old:dev": "parcel src/index.html",
    "build": "parcel build src/main.js -d dist/bundle --no-source-maps",
    "autobuild": "parcel watch src/main.js -d public/bundle --public-url .",
    "dev:sirv": "run-p autobuild sirv:dev",
    "dev:serve": "run-p serve:dev autobuild",
    "sirv:dev": "sirv public --dev --port 1234",
    "sirv:dist": "sirv dist --port 1234",
    "serve:dev": "serve public -d",
    "serve:dist": "serve dist -d"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "babel-preset-solid": "^0.17.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "serve": "^11.3.0",
    "sirv-cli": "^0.4.5",
    "solid-js": "^0.17.1"
  }
}
